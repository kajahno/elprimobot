{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-restricted-syntax": ["off", "ForOfStatement", "ContinueStatement"],
        "no-continue": "off",
        "no-plusplus": "off",
        "consistent-return": "off",
        "no-await-in-loop": "off",
        "no-multi-assign": "off",
        "prefer-template": "off",
        "import/extensions": [ "error", "always", { "ignorePackages": true} ],
        "guard-for-in": "off",
        "no-underscore-dangle": 0,
        "indent": ["error", 4],
        "quotes": [
            "error",
            "double"
        ],
        "camelcase": [
            "error", { 
                "properties": "never",
                "ignoreDestructuring": true,
                "ignoreImports": true
            }
        ]
    }
}
