{
  "name": "elprimobot",
  "version": "1.0.0",
  "description": "",
  "main": "es5-packaged/app.js",
  "scripts": {
    "start": "tsc && node es5-packaged/app.js",
    "prodstart": "./node_modules/.bin/eslint . && tsc --watch app.ts",
    "build": "./node_modules/.bin/eslint . && tsc --build",
    "package": "./node_modules/.bin/pkg -t node18-linuxstatic-x64,node18-win-x64 -o elprimobot es5-packaged/app.js",
    "deploy": "rsync  -avz elprimobot-linuxstatic elprimobot:~/elprimobot/",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint --fix ."
  },
  "bin": {
    "elprimobot": "es5-packaged/app.js"
  },
  "prettier": {
    "singleQuote": true
  },
  "author": "Karl Jahn",
  "license": "MIT",
  "dependencies": {
    "cron": "^2.0.0",
    "discord-interactions": "^3.2.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.7",
    "typescript": "^5.3.3",
    "winston": "^3.8.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.26.0",
    "pkg": "^5.8.0"
  }
}
